{
    "name": "joergtimm/uthlede",
    "type": "project",
    "description":"Web CMS from Joerg Timm",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "connectholland/cookie-consent-bundle": "^0.9.11",
        "doctrine/annotations": "^1.10",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "*",
        "endroid/qr-code-bundle": "^3.4",
        "friendsofsymfony/ckeditor-bundle": "^2.3",
        "gedmo/doctrine-extensions": "^3.0",
        "knplabs/knp-paginator-bundle": "^5.6",
        "knplabs/knp-time-bundle": "^1.16",
        "liip/imagine-bundle": "^2.3",
        "oneup/flysystem-bundle": "^4.0",
        "select2/select2": "^4.0",
        "sensio/framework-extra-bundle": "^5.6",
        "stof/doctrine-extensions-bundle": "^1.5",
        "symfony/asset": "5.*",
        "symfony/console": "5.*",
        "symfony/dotenv": "5.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.*",
        "symfony/framework-bundle": "5.*",
        "symfony/mailer": "5.*",
        "symfony/maker-bundle": "^1.34",
        "symfony/monolog-bundle": "^3.6",
        "symfony/security-bundle": "5.*",
        "symfony/security-csrf": "5.*",
        "symfony/sendgrid-mailer": "5.*",
        "symfony/ux-chartjs": "^1.2",
        "symfony/ux-dropzone": "^1.2",
        "symfony/ux-turbo": "^1.3",
        "symfony/ux-turbo-mercure": "^1.3",
        "symfony/ux-twig-component": "1.4.x-dev",
        "symfony/validator": "5.*",
        "symfony/webpack-encore-bundle": "^1.11",
        "symfony/yaml": "5.*",
        "symfonycasts/verify-email-bundle": "^1.0",
        "tetranz/select2entity-bundle": "^3.1",
        "twbs/bootstrap": "5.0.0-beta3",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "symfony/debug-bundle": "^5.2",
        "symfony/stopwatch": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/var-dumper": "^5.2",
        "symfony/web-profiler-bundle": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.*"
        }
    }
}
