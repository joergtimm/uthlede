<form method="post" action="{{ path('admin_artikel:delete', {'id': articel.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ articel.id) }}">
    <button class="btn btn-sm btn-danger me-2">
        <i class="fa fa-trash"></i>
    </button>
</form>
